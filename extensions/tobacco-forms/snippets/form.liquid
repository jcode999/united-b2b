<!-- blocks/custom-registration-form.liquid -->
<div id="custom-form-container"></div>
<script>
  document.addEventListener("DOMContentLoaded", function() {
    fetch("http://127.0.0.1:5500/temp/index.html")
      .then(response => response.text())
      .then(html => {
        document.getElementById("custom-form-container").innerHTML = html;
      })
      .catch(error => console.error('Error loading custom form:', error));
  });
</script>
